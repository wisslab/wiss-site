@inproceedings{Pfeffer2013c,
abstract = {Bei der Auswahl einer Aufstellungssystematik wird - neben anderen Kriterien - verst{\"{a}}rkt auf die Verbreitung des zugrundeliegenden Klassifikationssystems an anderen Bibliotheken geachtet. Die {\"{U}}berlegung ist dabei, dass die anderen Nutzer bereits eine gro{\ss}e Zahl an Buchtiteln klassifiziert haben und auf diese Daten f{\"{u}}r die eigene Klassifikationsarbeit zur{\"{u}}ckgegriffen werden kann. Im deutschsprachigen Raum ist es die Regensburger Verbundklassifikation (RVK), die durch die Nutzung im Bibliotheksverbund Bayern und die steigende Anwenderzahl einen gro{\ss}en Anteil an Fremddaten verspricht. 

In diesem Aufsatz werden wir am Beispiel des seit 2001 laufenden Projekts der Aufstellung der Freihandbest{\"{a}}nde der Universit{\"{a}}tsbibliothek Mannheim nach der RVK zun{\"{a}}chst aufzeigen, wie die in den deutschen Verb{\"{u}}nden verteilt vorhandenen Erschlie{\ss}ungsinformationen zusammengef{\"{u}}hrt werden k{\"{o}}nnen. Im zweiten Teil des Aufsatzes stellen wir eine speziell f{\"{u}}r die Bed{\"{u}}rfnisse einer schnellen und effizienten (Retro-)Klassifikation gro{\ss}er Best{\"{a}}nde entwickelte Software vor, die auf einer solch konsolidierten Datenbasis aufbaut.
},
address = {Hildesheim},
author = {Pfeffer, Magnus and Eckert, Kai},
booktitle = {Bibliotheken: Tore zur Welt des Wissens. 101. Deutscher Bibliothekartag in Hamburg 2012. Kongressband},
editor = {{Ulrich Hohoff} and {Klaus-Rainer Brintzinger} and {Benjamin R{\"{u}}cker}},
isbn = {978-3487148885},
language = {ger},
publisher = {Olms},
title = {{Jetzt kommt zusammen, was zusammen geh{\"{o}}rt: Verbund{\"{u}}bergreifende Datenkonsolidierung und wie man damit gro{\ss}e Best{\"{a}}nde effizient retroklassifiziert}},
year = {2013}
}
