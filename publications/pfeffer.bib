journal = {B.I.T. online},
keywords = {Bibliothekskatalog,Country code,Faceted browsing,Facettiertes Browsing,Geographic facet,Geographic search,Geographische Facette,Geographische Recherche,Geographischer Raum,Geographisches Schlagwort,Heidelberg University Library,Library catalog,Mannheim University Library,SWD,Schlagwortnormdatei,Subject authority file,Universit{\"{a}}tsbibliothek Heidelberg,Universit{\"{a}}tsbibliothek Mannheim},
number = {3},
pages = {245--252},
title = {{Mehr aus der Schlagwortnormdatei herausholen : Implementierung einer geographischen Facette in den Online-Katalogen der UB Heidelberg und der UB Mannheim}},
url = {http://archiv.ub.uni-heidelberg.de/volltextserver/frontdoor.php?source_opus=12555 http://www.b-i-t-online.de/heft/2011-03/fachbeitraege-wiesenmueller.pdf},
