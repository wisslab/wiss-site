@ARTICLE{Eckert2008a,
  author = {Eckert, Kai and Pfeffer, Magnus and Stuckenschmidt, Heiner},
  title = {{Assessing Thesaurus-Based Annotations for Semantic Search Applications}},
  journal = {International Journal on Metadata, Semantics and Ontologies},
  year = {2008},
  volume = {3},
  pages = {53-67},
  number = {1},
  abstract = {Statistical methods for automated document indexing are becoming an
	alternative to the manual assignment of keywords. We argue that the
	quality of the thesaurus used as a basis for indexing in regard to
	its ability to adequately cover the contents to be indexed and as
	a basis for the specific indexing method used is of crucial importance
	in automatic indexing. We present an interactive tool for thesaurus
	evaluation that is based on a combination of statistical measures
	and appropriate visualisation techniques that supports the detection
	of potential problems in a thesaurus. We describe the methods used
	and show that the tool supports the detection and correction of errors,
	leading to a better indexing result.},
